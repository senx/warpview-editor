{
  "name": "@senx/warpview-editor",
  "version": "1.0.20",
  "description": "WarpScript editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/senx/warpview-editor.git"
  },
  "author": "SenX <contact@senx.io>",
  "license": "Apache-2.0",
  "keywords": [
    "web",
    "editor",
    "code",
    "web components",
    "time series",
    "warp10",
    "Warp 10",
    "WarpScript",
    "Geo Times Series",
    "Times Series"
  ],
  "module": "dist/esm/index.js",
  "main": "dist/warpview-editor.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "sd concurrent \"node --max_old_space_size=4096 --optimize_for_size --stack_size=4096 ./node_modules/.bin/stencil build --prod\"",
    "dev": "sd concurrent \"node --max_old_space_size=4096 --optimize_for_size --stack_size=4096 ./node_modules/.bin/stencil build --dev --watch\" \"stencil-dev-server\"",
    "serve": "stencil-dev-server",
    "start": "stencil build --dev --watch --serve",
    "test": "jest --no-cache",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@giwisoft/wc-tabs": "^0.0.1",
    "@stencil/core": "^0.8.0",
    "monaco-editor": "0.13.1",
    "tslint": "^5.12.1",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@stencil/dev-server": "0.0.18",
    "@stencil/sass": "^0.0.6",
    "@stencil/utils": "0.0.4",
    "@timkendrick/monaco-editor": "0.0.9",
    "@types/jest": "^21.1.1",
    "@types/node": "^10.12.25",
    "jest": "^21.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-stencil": "^1.0.0"
  },
  "bugs": {
    "url": "https://github.com/senx/warpview-editor/issues"
  },
  "homepage": "https://www.warp10.io",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
